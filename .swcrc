{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/domain/*": ["src/domain/*"],
      "@/application/*": ["src/application/*"],
      "@/infrastruture/*": ["src/infrastruture/*"],
      "@/presentation/*": ["src/presentation/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
